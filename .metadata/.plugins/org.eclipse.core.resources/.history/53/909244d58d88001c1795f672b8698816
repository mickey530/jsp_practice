<%
	String session_id = request.getParameter("id");
	String session_pw = request.getParameter("pw");
	String session_nick = request.getParameter("nick");
	

	if(session_id.equals("kkk1234") && session_pw.equals("1111")){
		session.setAttribute("id", session_id);
		session.setAttribute("pw", session_pw);
		session.setAttribute("nick", session_nick);
		response.sendRedirect("session_welcome.jsp");
	}
%>
