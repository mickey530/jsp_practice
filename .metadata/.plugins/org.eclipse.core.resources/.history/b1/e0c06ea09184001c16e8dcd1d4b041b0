List<Integer> lottoList = new ArrayList<>();
%>
<%
while(lottoList.size() < 6){
	int lottoNum = (int)(Math.random()*45 + 1);
	if(!lottoList.contains(lottoNum)){
		lottoList.add(lottoNum);
		out.println(lottoNum);
	}

}
Collections.sort(lottoList);
%>

